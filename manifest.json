{
  "manifest_version": 3,
  "name": "Chrono01",
  "version": "1.1",
  "description": "Une extension Chrome qui fait la somme des heures.",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://emargement.zone01normandie.org/dashboard"],
      "js": ["dist/script/main.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "src/assets/chrono01.png",
    "48": "src/assets/chrono01.png",
    "128": "src/assets/chrono01.png"
  }
}
